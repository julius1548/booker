{
    "name": "bookkeeper/booker",
    "description": "description",
    "minimum-stability": "stable",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "bookkeeper/interface": "*"
    },
    "autoload": {
        "psr-4": {
            "Bookkeeper\\Booker\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bookkeeper\\Booker\\Providers\\BookerServiceProvider"
            ]
        }
    }
}
